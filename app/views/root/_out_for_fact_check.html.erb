<table class="table table-striped table-bordered table-condensed" summary="List of &ldquo;Fact Check Requested&rdquo; content">
  <thead>
    <tr class="table-header">
      <th scope="col" class="column-format"><%= sortable "_type", "Format" %></th>
      <th scope="col" class="column-title"><%= sortable "title" %></th>
      <th scope="col" class="column-updated"><%= sortable "updated_at", "Updated" %></th>
      <th scope="col" class="column-sent-out"><%= sortable "last_fact_checked_at", "Sent out" %></th>
      <th scope="col" class="column-assignee"><%= sortable "assignee", "Assigned to" %></th>
      <th scope="col" class="column-actions">Actions</th>
    </tr>
  </thead>
  <tbody>
    <%= render :collection => @presenter.fact_check, :partial => 'publication', :locals => {:tab => :fact_check, :with => with} %>
  </tbody>
</table>
